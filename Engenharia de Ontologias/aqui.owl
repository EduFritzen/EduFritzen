    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Campus_Santa_Maria -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Campus_Santa_Maria">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Universidade_Federal_de_Santa_Maria"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Faz_Parte_De"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Universidade_Federal_de_Santa_Maria"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">O campus Santa Maria da UFSM é uma unidade acadêmica da universidade localizada no bairro Camobi, na cidade de Santa Maria, no estado do Rio Grande do Sul, Brasil. É um dos quatro campi da UFSM.</rdfs:comment>
        <rdfs:label xml:lang="pt">Campus Santa Maria</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Ciência_da_Computação -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Ciência_da_Computação">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Campus_Santa_Maria"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Esta_Presente_Em"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Campus_Santa_Maria"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">Ciência da Computação é um curso do campus Santa Maria da UFSM.</rdfs:comment>
        <rdfs:label xml:lang="pt">Ciência da Computação</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Disciplinas -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Disciplinas">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Ciência_da_Computação"/>
        <rdfs:comment xml:lang="pt">Disciplinas são as matérias que compõem um curso de ensino superior. Elas são divididas em semestres e os estudantes precisam cumprir um determinado número de disciplinas para concluir o curso.</rdfs:comment>
        <rdfs:label xml:lang="pt">Disciplinas</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Integralização -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Integralização">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_Pedagógico"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#É_Especificado_Por"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_Pedagógico"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">O processo de integralização do Projeto Pedagógico do Curso de Ciência da Computação da UFSM é o processo pelo qual o estudante conclui todas as disciplinas necessárias para obtenção do diploma. Isso inclui cumprir os requisitos de créditos, além de atender as exigências específicas do curso, como ACGs, trabalhos de conclusão de curso, entre outros.</rdfs:comment>
        <rdfs:label xml:lang="pt">Integralização</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_Pedagógico -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_Pedagógico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Ciência_da_Computação"/>
        <rdfs:comment xml:lang="pt">O Projeto Pedagógico do Curso de Ciência da Computação da UFSM é um documento que estabelece os objetivos, metas e conteúdos do curso, assim como as estratégias de ensino e avaliação. Ele também descreve as habilidades e competências que os estudantes devem desenvolver ao longo do curso, e como essas habilidades e competências serão avaliadas.</rdfs:comment>
        <rdfs:label xml:lang="pt">Projeto Pedagógico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Qualquer_Semestre -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Qualquer_Semestre">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Disciplinas"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Código"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Carga_Horária"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">120</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Créditos"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">5</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">Qualquer Semestre é composto de várias disciplinas que os estudantes precisam cumprir para avançar no curso e que podem ser realizadas em qualquer semestre.</rdfs:comment>
        <rdfs:label xml:lang="pt">Qualquer Semestre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Universidade_Federal_de_Santa_Maria -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Universidade_Federal_de_Santa_Maria">
        <rdfs:comment xml:lang="pt">A Universidade Federal de Santa Maria (UFSM) é uma instituição de ensino superior pública localizada na cidade de Santa Maria, no estado do Rio Grande do Sul, no Brasil. Fundada em 1960, a UFSM oferece uma ampla variedade de cursos de graduação e pós-graduação nas áreas de ciências humanas, ciências exatas e ciências da natureza.</rdfs:comment>
        <rdfs:label xml:lang="pt">Universidade Federal de Santa Maria</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#1º_Semestre -->

    <owl:Class rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#1º_Semestre">
