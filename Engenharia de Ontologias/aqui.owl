    <!-- http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_de_Software_I -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Projeto_de_Software_I">
        <rdf:type rdf:resource="http://www.semanticweb.org/eduar/ontologies/2022/9/cc-ufsm-ontology#Qualquer_Semestre"/>
        <Carga_Horária rdf:datatype="http://www.w3.org/2001/XMLSchema#int">120</Carga_Horária>
